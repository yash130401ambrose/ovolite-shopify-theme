{{ 'custom-content.css' | asset_url | stylesheet_tag }}
{% liquid
  assign scrollMobile = section.settings.use_scroll_mobile

  assign container = section.settings.container
  if container == 'w-full'
    assign container = 'container-full'
  endif

  assign classes = 'm-section m-custom-content' | append: ' m-gradient m-color-' | append: section.settings.color_scheme
  if section.settings.custom_class != blank
    assign classes = classes | append: ' ' | append: section.settings.custom_class
  endif

  assign background_image = section.settings.background_image
  assign background_image_mobile = section.settings.background_image_mobile
  assign background_image_class = ''
  if background_image_mobile == blank
    assign background_image_mobile = background_image
  else
    assign background_image_class = 'm:hidden md:m:block'
  endif

  assign image_height = background_image.width | divided_by: background_image.aspect_ratio | round
  assign mobile_image_height = background_image_mobile.width | divided_by: background_image_mobile.aspect_ratio | round

  assign enable_parallax = section.settings.enable_parallax | default: false
%}
<style>
  #m-custom-{{ section.id }} {
    --column-gap: {{ section.settings.gap }}px;
    --column-gap-mobile: {{ section.settings.gap_mobile }}px;
    --section-padding-top: {{ section.settings.padding_top }}px;
    --section-padding-bottom: {{ section.settings.padding_bottom }}px;
  }
</style>
{% if enable_parallax %}
  <script src="{{ 'parallax.js' | asset_url }}" defer="defer"></script>
{% endif %}
<div
  id="m-custom-{{ section.id }}"
  class="{{ classes }}"
  data-section-type="custom-content"
  data-section-id="{{ section.id }}"
>
  {% if background_image != blank or background_image_mobile != blank %}
    {% liquid
      assign loading = 'lazy'
      assign fetchpriority = 'low'
      if section.index < 2 or section.settings.enable_preload_image
        assign loading = 'eager'
        assign fetchpriority = 'high'
      endif
    %}
    <m-parallax
      class="m-parallax m:block"
      data-enable-parallax="{{ enable_parallax }}"
      data-direction="{{ section.settings.parallax_direction }}"
    >
      <div class="m-parallax__wrapper m:relative m:w-full m:h-full">
        <div class="m-parallax__image m:h-full">
          {% if background_image != blank %}
            {{
              background_image
              | image_url: width: 3840
              | image_tag:
                class: background_image_class,
                loading: loading,
                fetchpriority: fetchpriority,
                width: background_image.width,
                height: image_height,
                sizes: '100vw',
                widths: '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840',
                alt: background_image.alt
              | escape
            }}
          {% endif %}
          {% if section.settings.background_image_mobile != blank %}
            {{
              background_image_mobile
              | image_url: width: 1500
              | image_tag:
                class: 'md:m:hidden',
                loading: loading,
                fetchpriority: fetchpriority,
                width: background_image_mobile.width,
                height: mobile_image_height,
                sizes: '100vw',
                widths: '375, 550, 750, 1100, 1500',
                alt: background_image_mobile.alt
              | escape
            }}
          {% endif %}
        </div>
      </div>
    </m-parallax>
  {% endif %}
  <div class="{{ container }} m-section-my m-section-py m-custom-content__container">
    <div class="m-gradient m-color-{{ section.settings.content_color_scheme }}{% if section.settings.content_color_scheme != section.settings.color_scheme %} m:blocks-radius m-custom-content__boxed{% endif %}">
      {%- assign block_width_mobile = '' -%}
      {% render 'index-section-header', section_settings: section.settings %}
      {% if section.blocks.size > 0 %}
        {% if scrollMobile %}<div class="m-mixed-layout m-mixed-layout--mobile-scroll">{% endif %}
        <div class="m-custom-content__wrapper m:flex {% unless scrollMobile %}m:flex-wrap{% else %}m-mixed-layout__inner md:m:flex-wrap{% endunless %}">
          {% for block in section.blocks %}
            {% liquid
              assign vertical_alignment = block.settings.alignment
              case block.settings.width
                when '16%'
                  assign block_width = 'lg:m:w-2/12'
                  assign block_width_mobile = 'md:w-1/2 m:w-full'
                when '25%'
                  assign block_width = 'lg:m:w-1/4'
                  assign block_width_mobile = 'md:m:w-1/2 m:w-full'
                when '33%'
                  assign block_width = 'lg:m:w-1/3'
                  assign block_width_mobile = 'm:w-full'
                when '50%'
                  assign block_width = 'lg:m:w-1/2'
                  assign block_width_mobile = 'm:w-full'
                when '66%'
                  assign block_width = 'lg:m:w-8/12'
                  assign block_width_mobile = 'm:w-full'
                when '75%'
                  assign block_width = 'lg:m:w-9/12'
                  assign block_width_mobile = 'm:w-full'
                when '83%'
                  assign block_width = 'lg:m:w-10/12'
                  assign block_width_mobile = 'm:w-full'
                when '100%'
                  assign block_width = 'lg:m:w-full'
                  assign block_width_mobile = 'm:w-full'
              endcase

              assign animation_effect = settings.animations
              assign animated = false
              if animation_effect != 'none'
                assign animated = true
                if block.settings.animations != 'inherit'
                  assign animation_effect = block.settings.animations
                endif
              endif

              assign heading_size = block.settings.text_size | replace: 'small', 'h3' | replace: 'medium', 'h2' | replace: 'large', 'h1'
              assign subtext_size = block.settings.text_size | replace: 'small', 'h6' | replace: 'medium', 'h6' | replace: 'large', 'h6'

              assign block_classes = 'm:column m:display-flex m-custom-content__block m-custom-content__block-' | append: block.type | append: ' ' | append: block_width | append: ' ' | append: block_width_mobile
              if vertical_alignment != blank
                assign block_classes = block_classes | append: ' m:items-' | append: vertical_alignment
              endif
              if block.settings.block_custom_class != blank
                assign block_classes = block_classes | append: ' ' | append: block.settings.block_custom_class
              endif

              assign text_color = block.settings.text_color
              assign heading_color = ''
              assign desc_color = 'm:text-color-subtext'
              if text_color != 'inherit'
                assign heading_color = heading_color | append: 'm:text-' | append: text_color
                assign desc_color = 'm:text-' | append: text_color
              endif
            %}
            <div id="m-custom__block--{{ block.id }}" class="{{ block_classes }}" {{ block.shopify_attributes }}>
              <div class="m-custom-content__block-inner m:w-full{% if animated and animation_effect == 'zoom-fade' %} m:overflow-hidden{% endif %}">
                {% case block.type %}
                  {% when 'image' %}
                    <div
                      class="m:w-full m:block{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %} m:blocks-radius"
                      {% if animated %}
                        data-cascade
                        style="--animation-order: {{ forloop.index }};"
                      {% endif %}
                    >
                      {% if block.settings.image != blank %}
                        {%- liquid
                          assign tag = 'div'
                          if block.settings.link
                            assign tag = 'a'
                          endif
                        -%}
                        <{{ tag }}
                          {% if block.settings.link %}
                            href="{{ block.settings.link }}"
                          {% endif %}
                        >
                          {% render 'responsive-image',
                            image: block.settings.image,
                            section_index: section.index,
                            index: 1,
                            enable_preload_image: block.settings.enable_preload_image
                          %}
                        </{{ tag }}>
                      {% else %}
                        {% if block.settings.width == '100%' %}
                          {{
                            'lifestyle-1'
                            | placeholder_svg_tag: 'placeholder-svg m-placeholder-svg m-placeholder-svg--no-border'
                          }}
                        {% else %}
                          {{
                            'image'
                            | placeholder_svg_tag: 'placeholder-svg m-placeholder-svg m-placeholder-svg--no-border'
                          }}
                        {% endif %}
                      {% endif %}
                    </div>
                  {% when 'text' %}
                    <div
                      class="m-richtext m-richtext--{{ block.settings.text_size }} m:text-{{ block.settings.align_text }}{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}"
                      style="--text-color: {{ text_color }};{% if animated %}--animation-order: {{ forloop.index }};{% endif %}"
                      {% if animated %}
                        data-cascade
                      {% endif %}
                    >
                      {% if block.settings.subheading != blank %}
                        <p class="m-richtext__subtitle m:text-{{ text_color }} {{ subtext_size }}">
                          {{ block.settings.subheading }}
                        </p>
                      {% endif %}
                      {% if block.settings.title != blank %}
                        <h3 class="m-richtext__title {{ heading_color }} {{ heading_size }}">
                          {{ block.settings.title }}
                        </h3>
                      {% endif %}
                      {% if block.settings.text != blank %}
                        <div class="m-richtext__description rte {{ desc_color }} {{ subtext_size }}">
                          {{ block.settings.text }}
                        </div>
                      {% endif %}
                      {% if block.settings.button_label != blank %}
                        <div class="m-richtext__button content__button">
                          <a
                            href="{{ block.settings.button_link }}"
                            class="m-button {{ block.settings.button_style | replace: 'sf__btn-primary', 'm-button--primary' | replace: 'sf__btn-secondary', 'm-button--secondary' | replace: 'sf__btn-link', 'm-button--link' | replace: 'sf__btn-white', 'm-button--white' }} {{ block.settings.button_size }}"
                          >
                            {{- block.settings.button_label -}}
                          </a>
                        </div>
                      {% endif %}
                    </div>
                  {% when 'video' %}
                    {{ 'video.css' | asset_url | stylesheet_tag }}
                    {% liquid
                      assign autoplay = false
                      if block.settings.autoplay and block.settings.muted
                        assign autoplay = true
                      endif

                      assign cover_image = false
                      if block.settings.image != blank
                        assign cover_image = true
                      endif
                    %}
                    <div
                      class="m-video{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}"
                      data-video-block
                      {% if animated %}
                        data-cascade
                        style="--animation-order: {{ forloop.index }};"
                      {% endif %}
                    >
                      <div class="m-video__container m-video__container--{{ block.id }} mx-auto">
                        <div
                          class="m-video__wrapper m-video__wrapper--{{ block.id }} m:blocks-radius"
                          style="--aspect-ratio: {{ block.settings.ratio | replace: ':', '/' }}"
                        >
                          <video-component
                            class="deferred-media no-js-hidden"
                            data-media-id="{{ block.settings.video_link.id }}"
                            data-auto-play="{{ autoplay }}"
                            data-muted="{{ block.settings.muted }}"
                            data-cover-image="{{ cover_image }}"
                          >
                            {% unless autoplay %}
                              <button
                                id="Video-Deferred-Poster-Modal-{{ block.settings.video_link.id }}"
                                class="deferred-media__poster"
                                type="button"
                                aria-label="Play"
                              >
                                <span class="deferred-media__poster-button motion-reduce m-video__play m-video__play--style-{{ block.settings.play_style }} m-video__play--size-{{ block.settings.play_size }} m:text-{{ block.settings.text_color }}">
                                  <svg viewBox="0 0 18 18" fill="currentColor">
                                    <path d="M15.562 8.1L3.87.225c-.818-.562-1.87 0-1.87.9v15.75c0 .9 1.052 1.462 1.87.9L15.563 9.9c.584-.45.584-1.35 0-1.8z" fill="currentColor"></path>
                                  </svg>
                                </span>
                                {%- unless block.settings.video_title == blank -%}
                                  <h2 class="m-video__title m:text-xl {% if block.settings.text_size == 'medium' %}sm:m:text-3xl{% else %}sm:m:text-5xl{% endif %} m:text-{{ block.settings.text_color }}">
                                    {{ block.settings.video_title }}
                                  </h2>
                                {%- endunless -%}
                                {%- if block.settings.image != blank -%}
                                  <div
                                    class="m-video__cover m-bg-lazy"
                                    style="background-image: url({{ block.settings.image | image_url: width: 1920 }})"
                                  ></div>
                                {%- else -%}
                                  <div class="m-video__cover m-video__cover-placeholder">
                                    {{ 'lifestyle-2' | placeholder_svg_tag: 'm-placeholder-svg m:w-full m:h-full' }}
                                  </div>
                                {%- endif -%}
                              </button>
                            {% endunless %}
                            <template>
                              {%- if block.settings.video_type == 'external' -%}
                                {%- if block.settings.video_link.type == 'youtube' -%}
                                  {% liquid
                                    assign youtubeURL = 'https://www.youtube.com/embed/' | append: block.settings.video_link.id | append: '?enablejsapi=1'

                                    if block.settings.muted
                                      assign youtubeURL = youtubeURL | append: '&mute=1'

                                      if block.settings.autoplay
                                        assign youtubeURL = youtubeURL | append: '&autoplay=1'
                                      endif
                                    endif

                                    if block.settings.loop
                                      assign youtubeURL = youtubeURL | append: '&loop=1&playlist=' | append: block.settings.video_link.id
                                    endif
                                  %}
                                  <iframe
                                    src="{{ youtubeURL }}"
                                    class="js-youtube"
                                    allow="autoplay; encrypted-media"
                                    allowfullscreen
                                    title="{{ block.settings.video_title | escape }}"
                                  ></iframe>
                                {%- else -%}
                                  {% liquid
                                    assign vimeoURL = 'https://player.vimeo.com/video/' | append: block.settings.video_link.id
                                    assign hasParam = false
                                    assign vimeoParam = ''

                                    if block.settings.muted
                                      assign hasParam = true
                                      assign vimeoParam = vimeoParam | append: '&muted=1'

                                      if block.settings.autoplay
                                        assign vimeoParam = vimeoParam | append: '&autoplay=1'
                                      endif
                                    endif

                                    if block.settings.loop
                                      assign hasParam = true
                                      assign vimeoParam = vimeoParam | append: '&loop=1'
                                    endif

                                    if hasParam
                                      assign vimeoURL = vimeoURL | append: '?' | append: vimeoParam
                                    endif
                                  %}
                                  <iframe
                                    src="{{ vimeoURL }}"
                                    class="js-vimeo"
                                    allow="autoplay; encrypted-media"
                                    allowfullscreen
                                    title="{{ block.settings.video_title | escape }}"
                                  ></iframe>
                                {%- endif -%}
                              {%- else -%}
                                {% liquid
                                  assign video_url = 'https://cdn.shopify.com/videos/c/o/v/fbb140fd46e3429eba459e1549dbbe38.mp4'
                                  for source in block.settings.shopify_video.sources
                                    assign format = source.format
                                    if format == 'mp4'
                                      assign video_url = source.url
                                      break
                                    endif
                                  endfor
                                %}
                                <video
                                  src="{{ video_url }}"
                                  playsinline
                                  {% if block.settings.show_controls %}
                                    controls
                                  {% endif %}
                                  {% if block.settings.loop %}
                                    loop
                                  {% endif %}
                                  {% if block.settings.muted %}
                                    muted
                                    {% if block.settings.autoplay %} autoplay{% endif %}
                                  {% endif %}
                                ></video>
                              {%- endif -%}
                            </template>
                          </video-component>
                        </div>
                      </div>
                    </div>
                  {% when 'product' %}
                    {{ 'component-product-inventory.css' | asset_url | stylesheet_tag }}
                    {%- assign product = block.settings.product -%}
                    {% if product.title.size > 0 %}
                      {% liquid
                        case settings.pcard_layout
                          when '1'
                            render 'product-card-1', product: product, index: forloop.index, animated: animated, animation_effect: animation_effect
                          when '2'
                            render 'product-card-2', product: product, index: forloop.index, animated: animated, animation_effect: animation_effect
                          when '3'
                            render 'product-card-3', product: product, index: forloop.index, animated: animated, animation_effect: animation_effect
                          when '4'
                            render 'product-card-4', product: product, index: forloop.index, animated: animated, animation_effect: animation_effect
                          when '5'
                            render 'product-card-5', product: product, index: forloop.index, animated: animated, animation_effect: animation_effect
                        endcase
                      %}
                    {% else %}
                      {% comment %}
                        No product yet. Show onboarding one.
                      {% endcomment %}
                      <div
                        class="m-product-card m-product-card--placeholder{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}"
                        {% if animated %}
                          data-cascade
                          style="--animation-order: {{ forloop.index }};"
                        {% endif %}
                      >
                        <div class="m-product-card__media" style="--aspect-ratio: 1/1;">
                          {% capture current %}{% cycle 1, 2, 3, 4, 5, 6 %}{% endcapture %}
                          {{
                            'product-'
                            | append: current
                            | placeholder_svg_tag: 'm-placeholder-svg m:object-cover m:w-full m:h-full'
                          }}
                        </div>
                        <div class="m-product-card__name{% if settings.uppercase_prd_name %} m:uppercase{% endif %}">
                          {{ 'homepage.onboarding.product_title' | t }}
                        </div>
                        <div class="m-product-card__price">
                          <p class="m-price">$19.99</p>
                        </div>
                      </div>
                    {% endif %}
                  {% when 'product_form' %}
                    {% liquid
                      assign product = block.settings.product
                      assign current_variant = product.selected_or_first_available_variant
                      assign section_onboarding = false
                      if product == empty
                        assign section_onboarding = true
                      endif
                    %}

                    <div
                      class="m-main-product--wrapper m-main-product--info"
                      data-section_onboarding="{{ section_onboarding }}"
                    >
                      <div
                        class="m-product-info--wrapper{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}"
                        {% if animated %}
                          data-cascade
                          style="--animation-order: {{ forloop.index }};"
                        {% endif %}
                      >
                        {% if block.settings.show_vendor %}
                          <div class="main-product__block main-product__block-vendor">
                            <span>{{ product.vendor }}</span>
                          </div>
                        {% endif %}
                        <div class="main-product__block main-product__block-title">
                          <a class="m-product-title m:text-2xl md:m:text-3xl m:font-medium" href="{{ product.url }}">
                            {% unless section_onboarding %}
                              {{ product.title }}
                            {% else %}
                              {{ 'homepage.onboarding.product_title' | t }}
                            {% endunless %}
                          </a>
                        </div>
                        {% if block.settings.show_price %}
                          <div class="main-product__block main-product__block-price">
                            {% unless section_onboarding %}
                              {% render 'product-prices', product: product, price_class: 'm:text-xl' %}
                            {% else %}
                              <span class="m:text-xl m:color-price-regular">{{ 1999 | money }}</span>
                            {% endunless %}
                          </div>
                        {% endif %}
                        {% if block.settings.show_product_review %}
                          <div class="main-product__block main-product__block-rating">
                            {% render 'product-reviews-app__badge', product: product %}
                          </div>
                        {% endif %}
                        {% unless section_onboarding %}
                          <div class="main-product__block main-product__block-description rte">
                            {{ product.description }}
                          </div>
                        {% else %}
                          <div class="main-product__block main-product__block-description rte">
                            {{ 'homepage.onboarding.product_description' | t }}
                          </div>
                        {% endunless %}
                        {% unless section_onboarding %}
                          {%- liquid
                            assign product_form_id = 'product-form-' | append: section.id
                            assign product_form_class = 'product-form-' | append: section.id
                          -%}
                          {% render 'product-form',
                            section_id: section.id,
                            product: product,
                            section: block,
                            product_form_id: product_form_id,
                            product_form_class: product_form_class,
                            current_variant: current_variant,
                            enable_dynamic_checkout: block.settings.enable_payment_button,
                            show_quantity_selector: block.settings.show_quantity_selector,
                            enableCardButton: block.settings.show_add_to_cart
                          %}
                        {% else %}
                          <div class="main-product__block main-product__block-buy_buttons">
                            <button type="submit" class="m-add-to-cart m-button m-button--primary m:w-full">
                              <span>{{ 'products.product.add_to_cart' | t }}</span>
                            </button>
                          </div>
                        {% endunless %}
                      </div>
                      {% if product.options_with_values.size > 0 %}
                        <script src="{{ 'variant-picker.js' | asset_url }}" defer="defer"></script>
                      {% endif %}
                    </div>
                  {% when 'image_card' %}
                    {{ 'component-image-card.css' | asset_url | stylesheet_tag }}
                    {% render 'image-card',
                      block: block,
                      grid_columns: 'm:w-full',
                      stack_on_mobile: scrollMobile,
                      image_card_class: 'm-hover-box m-hover-box--scale-up',
                      index: 1,
                      section_index: section.index,
                      enable_preload_image: block.settings.enable_preload_image,
                      animated: animated,
                      animation_effect: animation_effect,
                      color_scheme: block.settings.color_scheme,
                      parent_color_scheme: section.settings.content_color_scheme
                    %}
                  {% when 'video_card' %}
                    {{ 'component-image-card.css' | asset_url | stylesheet_tag }}
                    {% render 'video-card',
                      block: block,
                      grid_columns: 'm:w-full',
                      stack_on_mobile: scrollMobile,
                      index: forloop.index,
                      animated: animated,
                      animation_effect: animation_effect,
                      color_scheme: block.settings.color_scheme,
                      parent_color_scheme: section.settings.content_color_scheme
                    %}
                  {% when 'product_bundles' %}
                    {{ 'component-product-bundles.css' | asset_url | stylesheet_tag }}
                    <script src="{{ 'product-bundles.js' | asset_url }}" defer="defer"></script>
                    <m-product-bundle
                      class="product-bundles{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}"
                      {% if animated %}
                        data-cascade
                        style="--animation-order: {{ forloop.index }};"
                      {% endif %}
                    >
                      <div class="m-richtext m-richtext--{{ block.settings.text_size | replace: 'h3', 'small' }}">
                        {% if block.settings.subheading != blank %}
                          <span class="m-richtext__subtitle m:block {{ block.settings.text_size | replace: 'h2', 'h5' | replace: 'h3', 'h6' }}">
                            {{- block.settings.subheading -}}
                          </span>
                        {% endif %}
                        {% if block.settings.title != blank %}
                          <h3 class="m-richtext__title {{ block.settings.text_size }}">{{ block.settings.title }}</h3>
                        {% endif %}
                      </div>
                      <div class="product-bundles__list">
                        {% liquid
                          assign products = 'product_1,product_2,product_3,product_4,product_5' | split: ','
                          assign total_price = 0
                          assign has_product = false
                          assign image_ratio = block.settings.image_ratio
                        %}
                        {% for product_id in products %}
                          {% liquid
                            assign saved_price = ''
                            assign product = block.settings[product_id]
                            assign variant = product.first_available_variant
                            if variant.compare_at_price != blank and variant.compare_at_price > variant.price
                              assign saved_price = variant.compare_at_price | minus: variant.price | money
                            endif
                            assign sold_out = false
                            unless variant.available
                              assign sold_out = true
                            endunless
                          %}
                          {% if product != empty %}
                            {% liquid
                              assign has_product = true
                              assign total_price = total_price | plus: variant.price
                              assign product_available = false

                              for variant in product.variants
                                if variant.available == true
                                  assign product_available = true
                                endif
                              endfor
                            %}
                            <div
                              class="product-bundles__item m:display-flex m:items-center{% if block.settings.show_reviews %} product-bundles__item--has-reviews{% endif %}"
                              data-index="{{ forloop.index0 }}"
                            >
                              <span class="product-bundles__item-index m:inline-flex m:justify-center m:items-center">
                                {{- forloop.index -}}
                              </span>
                              <a
                                href="{{ product.url }}"
                                class="m:block product-bundles__item-image m:blocks-radius-md"
                              >
                                {% render 'responsive-image',
                                  image: product.featured_image,
                                  aspect_ratio: image_ratio
                                %}
                              </a>
                              <div class="product-bundles__item-info m:display-flex m:flex-wrap md:m:flex-nowrap">
                                <div class="product-bundles__item-details m:w-full">
                                  {% if block.settings.show_reviews %}
                                    <div class="product-bundles__item-review m:text-sm">
                                      {% render 'product-reviews-app__badge', product: product %}
                                    </div>
                                  {% endif %}
                                  <h3 class="product-bundles__item-title h6">
