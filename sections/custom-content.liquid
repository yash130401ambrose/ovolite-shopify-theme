                          assign image_class = ''
                          if image_mobile != blank
                            assign image_class = image_class | append: ' has-mobile-image'
                          endif
                          assign loading = 'lazy'
                          assign fetchpriority = 'low'
                          if section.index < 2
                            assign loading = 'eager'
                            assign fetchpriority = 'high'
                          endif
                        %}
                        {% if animated %}<span class="m-image-comparison__slider-animation"></span>{% endif %}
                        <div class="m-image-comparison__before m-image-comparison--media">
                          {% if heading != blank %}
                            <p class="m-image-comparison__heading h6 m:blocks-radius-md">{{ heading }}</p>
                          {% endif %}
                          <div
                            class="m-image-comparison__wrapper m-image-comparison--{{ image_height }}{{ image_class }}"
                            {% if image_height == 'adapt' %}
                              style="--aspect-ratio: {{ aspect_ratio }}; --aspect-ratio-mobile: {{ aspect_ratio_mobile }}"
                            {% endif %}
                          >
                            {% liquid
                              assign image_alt = image.alt | default: 'image comparison before'
                            %}
                            {% if image != blank %}
                              {% assign image_height = image.width | divided_by: image.aspect_ratio %}
                              {{
                                image
                                | image_url: width: 1500
                                | image_tag:
                                  loading: loading,
                                  fetchpriority: fetchpriority,
                                  class: 'm-image-comparison__image-desktop',
                                  width: image.width,
                                  height: image_height,
                                  sizes: '100vw',
                                  widths: '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840',
                                  alt: image_alt
                              }}
                            {% else %}
                              {{
                                'lifestyle-1'
                                | placeholder_svg_tag: 'm-placeholder-svg m-image-comparison__image-desktop'
                              }}
                            {% endif %}
                            {% if image_mobile != blank %}
                              {% assign image_mobile_height = image_mobile.width
                                | divided_by: image_mobile.aspect_ratio
                              %}
                              {{
                                image_mobile
                                | image_url: width: 750
                                | image_tag:
                                  loading: loading,
                                  fetchpriority: fetchpriority,
                                  class: 'm-image-comparison__image-mobile',
                                  width: image_mobile.width,
                                  height: image_mobile_height,
                                  sizes: '100vw',
                                  widths: '375, 550, 750',
                                  alt: image_mobile.alt
                                | escape
                              }}
                            {% endif %}
                          </div>
                        </div>

                        {% liquid
                          assign image = block.settings.after_image
                          assign image_mobile = block.settings.after_image_mobile
                          assign heading = block.settings.after_heading
                          assign image_class = ''
                          if image_mobile != blank
                            assign image_class = image_class | append: ' has-mobile-image'
                          endif
                        %}
                        <div class="m-image-comparison__after m-image-comparison--media">
                          {% if heading != blank %}
                            <p class="m-image-comparison__heading h6 m:blocks-radius-md">{{ heading }}</p>
                          {% endif %}
                          <div
                            class="m-image-comparison__wrapper m-image-comparison--{{ image_height }}{{ image_class }}"
                            {% if image_height == 'adapt' %}
                              style="--aspect-ratio: {{ aspect_ratio }}; --aspect-ratio-mobile: {{ aspect_ratio_mobile }}"
                            {% endif %}
                          >
                            {% liquid
                              assign image_alt_after = image.alt | default: 'image comparison after'
                            %}
                            {% if image != blank %}
                              {% assign image_height = image.width | divided_by: image.aspect_ratio %}
                              {{
                                image
                                | image_url: width: 1500
                                | image_tag:
                                  loading: loading,
                                  fetchpriority: fetchpriority,
                                  class: 'm-image-comparison__image-desktop',
                                  width: image.width,
                                  height: image_height,
                                  sizes: '100vw',
                                  widths: '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840',
                                  alt: image_alt_after
                              }}
                            {% else %}
                              {{
                                'lifestyle-2'
                                | placeholder_svg_tag: 'm-placeholder-svg m-image-comparison__image-desktop'
                              }}
                            {% endif %}
                            {% if image_mobile != blank %}
                              {% assign image_mobile_height = image_mobile.width
                                | divided_by: image_mobile.aspect_ratio
                              %}
                              {{
                                image_mobile
                                | image_url: width: 750
                                | image_tag:
                                  loading: loading,
                                  fetchpriority: fetchpriority,
                                  class: 'm-image-comparison__image-mobile',
                                  width: image_mobile.width,
                                  height: image_mobile_height,
                                  sizes: '100vw',
                                  widths: '375, 550, 750',
                                  alt: image_mobile.alt
                                | escape
                              }}
                            {% endif %}
                          </div>
                        </div>

                        <button class="m-image-comparison__button" aria-label="Drag">
                          <span>
                            <svg
                              xmlns="http://www.w3.org/2000/svg"
                              aria-hidden="true"
                              focusable="false"
                              fill="none"
                              viewBox="0 0 11 16"
                            >
                              <path fill-rule="evenodd" clip-rule="evenodd" d="M0 0.5C0 0.223858 0.223858 0 0.5 0C0.776142 0 1 0.223858 1 0.5V15.5C1 15.7761 0.776142 16 0.5 16C0.223858 16 0 15.7761 0 15.5V0.5ZM5 0.5C5 0.223858 5.22386 0 5.5 0C5.77614 0 6 0.223858 6 0.5V15.5C6 15.7761 5.77614 16 5.5 16C5.22386 16 5 15.7761 5 15.5V0.5ZM11 0.5C11 0.223858 10.7761 0 10.5 0C10.2239 0 10 0.223858 10 0.5V15.5C10 15.7761 10.2239 16 10.5 16C10.7761 16 11 15.7761 11 15.5V0.5Z" fill="currentColor"></path>
                            </svg>
                          </span>
                        </button>
                      </m-image-comparison>
                      {% if product != blank %}
                        <div class="m-image-comparison__product{% if animated %} m-scroll-trigger animate--{{ animation_effect }}{% endif %}">
                          <h4 class="h6 m-image-comparison__product-title">
                            <a class="m-link-underline" href="{{ product.url }}">{{ product.title }}</a>
                          </h4>
                          <div class="m-image-comparison__product-price">
                            {% render 'product-prices', product: product %}
                          </div>
                        </div>
                      {% endif %}
                    </div>
                {% endcase %}
              </div>
            </div>
          {% endfor %}
        </div>
        {% if scrollMobile %}</div>{% endif %}
      {% endif %}
    </div>
  </div>
</div>

